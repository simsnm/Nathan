# .env.example - Copy to .env and add your actual API keys
# NEVER commit the actual .env file!

# AI Provider API Keys (get from respective providers)
ANTHROPIC_API_KEY=sk-your_anthropic_key_here
OPENAI_API_KEY=sk-your_openai_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Development Settings
CORS_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000", "http://localhost:8000", "http://127.0.0.1:8000"]
DEBUG=true
API_HOST=0.0.0.0
API_PORT=8000

# Optional: Logging
LOG_LEVEL=INFO
LOG_FILE=codechat.log

# Optional: Session Storage
SESSION_TIMEOUT=3600
MAX_FILE_SIZE=10485760

# Optional: Rate Limiting
REQUESTS_PER_MINUTE=60